            name="nameCardDelId"
            column="nameCardDelId"
            type="java.lang.Long"
            length="19"
        >
			<generator class="native"/>
        </id>
         <!--<property
            name="cardNum"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="cardNum"
            length="128"
            not-null="true"
            unique="false"
        />
         <many-to-one name="shop" class="com.xpsoft.oa.model.system.Department" not-null="false"/>
		<many-to-one name="shoppingGuid"	class="com.xpsoft.oa.model.system.AppUser" not-null="false" lazy="true"/>
		<property
            name="type"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="type" 
            not-null="true"
            unique="false"
        />
        <property
            name="refundAmount"
            type="java.math.BigDecimal"
            update="true"
