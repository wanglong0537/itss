<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.info.NoticeNewsComment"
        table="notice_news_comment"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="commentId"
            column="commentId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="content"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="content"
            length="500"
            not-null="true"
            unique="false"
        />
        <property
            name="createtime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createtime"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="fullname"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="fullname"
            length="32"
            not-null="true"
            unique="false"
        />
        <property
            name="flag"
            type="java.lang.Integer"
            update="false"
            insert="true"
            access="property"
            column="flag"
            length="32"
            not-null="true"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to News -->
		<many-to-one
			name="news"
			class="com.xpsoft.oa.model.info.NoticeNews"
            not-null="false"
			fetch="select"
		>	
			<column name="newsId" />
		</many-to-one>
    <!-- imported bi-directional many-to-one association to AppUser -->
		<many-to-one
			name="appUser"
			class="com.xpsoft.oa.model.system.AppUser"
            not-null="false"
			fetch="select"
		>	
			<column name="userId" />
		</many-to-one>
    </class>
</hibernate-mapping>
